  "version": "0.1.0",
  "description": "A React 19 SSR framework with streaming support and edge runtime compatibility",
  "type": "commonjs",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
